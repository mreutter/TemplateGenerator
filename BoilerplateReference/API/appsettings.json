$!={
{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
!¬$?useDBConnectionString#
!¬$?useAuthentication#
}}#

$?useDBConnectionString={
,
    "ConnectionStrings": {
        "DefaultConnection": "server=$%dbHost#;uid=$%dbUser#;pwd=$%dbPassword#;database=$%dbName#;"
   }
!¬}#

$?useAuthentication={
,
    "Jwt": {
        "Key": "$%key#",
        "Issuer": "$%issuer#",
        "Audience": "$%audience#"
    }
!¬}#