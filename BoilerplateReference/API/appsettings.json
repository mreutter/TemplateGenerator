$!={
{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
!¬$?useDbConnectionString#
!¬$?useAuth#
}}#

$?useDbConnectionString={
,
    "ConnectionStrings": {
        "DefaultConnection": "server=$%dbHost#;uid=$%dbUser#;pwd=$%dbPassword#;database=$%dbName#;"
   }
!¬}#

$?useAuth={
,
    Jwt: {
        Key: $%key#,
        Issuer: $%issuer#,
        Audience: $%audience#
    }
!¬}#